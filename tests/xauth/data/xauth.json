{
  "test_Proxy": [
    {
        "name": "Google",
        "request_body": {
          "api_call_url": "https://www.google.com/?gws_rd=ssl"
        },
        "asserts": [
          {
            "response": 200
          }
        ]
      },

      {
        "name": "Facebook",
        "request_body": {
          "api_call_url": "https://www.facebook.com"
        },
        "asserts": [
          {
            "response": 200
          }
        ]
      },

      {
        "name": "Bad Domain",
        "request_body": {
          "api_call_url": "www.asdfasdfsdfs.com"
        },
        "asserts": [
          {
            "response": 400
          }
        ]
      },

      {
        "name": "Empty domain",
        "request_body": {
          "api_call_url": ""
        },
        "asserts": [
          {
            "response": 400
          }
        ]
      }
  ]
}