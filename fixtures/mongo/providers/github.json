{
  "provider": {
    "provider_number": "000000000000000000000007",
    "name": "GitHub",
    "client_callable": true,
    "domain": "https://api.github.com",
    "endpoint_wait_time": 0,
    "backend_name": "github",
    "auth_backend": "social.backends.github.GithubOAuth2",
    "description": "Web-based Git repository hosting service",
    "tags": [
      "collaboration",
      "development",
      "productivity"
    ],
    "auth_type": 0
  },
  "event_sources": {
    "events": {
      "display_name": "GitHub Events",
      "description": "Events in your GitHub history, including Pushes (which contain commits), Pull Requests, Forks, and Branch creation",
      "enabled_by_default": true,
      "endpoints": [
        "events",
        "repo"
      ],
      "initial_mapping": "events",
      "mappings": {
        "events": {
          "returned_data_location": "data",
          "event_type": "edit",
          "db_entry_mapping": {
            "data": {
              "main": {
                "unique_identifiers": [
                  {
                    "hydration_method": "translate",
                    "value": "permission.event_source.name"
                  },
                  {
                    "hydration_method": "translate",
                    "value": "responseObject.id"
                  }
                ]
              }
            },
            "event": {
              "unique_identifiers": [
                {
                  "hydration_method": "literal",
                  "value": "event"
                },
                {
                  "hydration_method": "translate",
                  "value": "responseObject.id"
                }
              ],
              "mapped_fields": {
                "contact_interaction_type": {
                  "hydration_method": "literal",
                  "value": "with"
                },
                "datetime": {
                  "hydration_method": "translate",
                  "type": "string",
                  "value": "responseObject.created_at"
                },
                "provider_name": {
                  "hydration_method": "literal",
                  "type": "string",
                  "value": "github"
                }
              }
            },
            "content": {
              "fork": {
                "conditions": [
                  {
                    "equals": {
                      "field": "responseObject.type",
                      "value": "ForkEvent"
                    }
                  }
                ],
                "unique_identifiers": [
                  {
                    "hydration_method": "literal",
                    "value": "fork"
                  },
                  {
                    "hydration_method": "translate",
                    "value": "responseObject.id"
                  }
                ],
                "mapped_fields": {
                  "content_type": {
                    "hydration_method": "literal",
                    "type": "string",
                    "value": "code"
                  },
                  "owner": {
                    "hydration_method": "translate",
                    "value": "responseObject.payload.forkee.owner.login"
                  },
                  "url": {
                    "hydration_method": "translate",
                    "value": "responseObject.payload.forkee.html_url"
                  },
                  "title": {
                    "hydration_method": "translate",
                    "value": "responseObject.payload.forkee.full_name"
                  }
                }
              },
              "pull_request": {
                "conditions": [
                  {
                    "equals": {
                      "field": "responseObject.type",
                      "value": "PullRequestEvent"
                    }
                  }
                ],
                "unique_identifiers": [
                  {
                    "hydration_method": "literal",
                    "value": "pull_request"
                  },
                  {
                    "hydration_method": "translate",
                    "value": "responseObject.id"
                  }
                ],
                "mapped_fields": {
                  "content_type": {
                    "hydration_method": "literal",
                    "type": "string",
                    "value": "code"
                  },
                  "owner": {
                    "hydration_method": "translate",
                    "value": "responseObject.payload.pull_request.head.user.login"
                  },
                  "url": {
                    "hydration_method": "translate",
                    "value": "responseObject.payload.pull_request.html_url"
                  },
                  "title": {
                    "hydration_method": "translate",
                    "value": "responseObject.payload.pull_request.title"
                  }
                }
              },
              "commit": {
                "conditions": [
                  {
                    "equals": {
                      "field": "responseObject.type",
                      "value": "PushEvent"
                    }
                  }
                ],
                "hydrate_list_location": "responseObject.payload.commits",
                "unique_identifiers": [
                  {
                    "hydration_method": "literal",
                    "value": "commit"
                  },
                  {
                    "hydration_method": "translate",
                    "value": "sha"
                  }
                ],
                "mapped_fields": {
                  "content_type": {
                    "hydration_method": "literal",
                    "type": "string",
                    "value": "code"
                  },
                  "owner": {
                    "hydration_method": "translate",
                    "value": "author.name"
                  },
                  "url": {
                    "hydration_method": "translate",
                    "value": "url"
                  },
                  "text": {
                    "hydration_method": "translate",
                    "value": "message"
                  },
                  "title": {
                    "hydration_method": "reformat",
                    "hydration_location": "sha",
                    "value": "Commit #[$1]"
                  }
                }
              },
              "branch": {
                "conditions": [
                  {
                    "equals": {
                      "field": "responseObject.type",
                      "value": "CreateEvent"
                    }
                  },
                  {
                    "equals": {
                      "field": "responseObject.payload.ref_type",
                      "value": "branch"
                    }
                  }
                ],
                "unique_identifiers": [
                  {
                    "hydration_method": "literal",
                    "value": "create_branch"
                  },
                  {
                    "hydration_method": "translate",
                    "value": "responseObject.id"
                  }
                ],
                "mapped_fields": {
                  "content_type": {
                    "hydration_method": "literal",
                    "type": "string",
                    "value": "code"
                  },
                  "owner": {
                    "hydration_method": "translate",
                    "value": "responseObject.actor.login"
                  },
                  "url": {
                    "hydration_method": "translate",
                    "value": "responseObject.repo.url"
                  },
                  "text": {
                    "hydration_method": "translate",
                    "value": "responseObject.payload.description"
                  },
                  "title": {
                    "hydration_method": "translate",
                    "value": "responseObject.payload.ref"
                  }
                }
              }
            },
            "contacts": {
              "push": {
                "hydrate_list_location": "responseObject.repo",
                "conditions": [
                  {
                    "equals": {
                      "field": "responseObject.type",
                      "value": "PushEvent"
                    }
                  }
                ],
                "unique_identifiers": [
                  {
                    "hydration_method": "literal",
                    "value": "github"
                  },
                  {
                    "hydration_method": "endpoint",
                    "value": {
                      "endpoint": "repo",
                      "parameter_sources": {
                        "repo_name": {
                          "hydration_method": "translate",
                          "value": "name"
                        }
                      },
                      "value_location": [
                        [
                          "owner.login"
                        ]
                      ]
                    }
                  }
                ],
                "mapped_fields": {
                  "api_id": {
                    "hydration_method": "endpoint",
                    "value": {
                      "endpoint": "repo",
                      "parameter_sources": {
                        "repo_name": {
                          "hydration_method": "translate",
                          "value": "name"
                        }
                      },
                      "value_location": [
                        [
                          "owner.id"
                        ]
                      ]
                    }
                  },
                  "handle": {
                    "hydration_method": "endpoint",
                    "value": {
                      "endpoint": "repo",
                      "parameter_sources": {
                        "repo_name": {
                          "hydration_method": "translate",
                          "value": "name"
                        }
                      },
                      "value_location": [
                        [
                          "owner.login"
                        ]
                      ]
                    }
                  }
                }
              },
              "commit": {
                "conditions": [
                  {
                    "equals": {
                      "field": "responseObject.type",
                      "value": "PushEvent"
                    }
                  }
                ],
                "hydrate_list_location": "responseObject.payload.commits",
                "unique_identifiers": [
                  {
                    "hydration_method": "literal",
                    "value": "github"
                  },
                  {
                    "hydration_method": "translate",
                    "value": "author.name"
                  }
                ],
                "mapped_fields": {
                  "handle": {
                    "hydration_method": "translate",
                    "value": "author.email"
                  },
                  "name": {
                    "hydration_method": "translate",
                    "value": "author.name"
                  }
                }
              },
              "pull_request": {
                "hydrate_list_location": "responseObject.payload.pull_request.base.user.repo.owner",
                "conditions": [
                  {
                    "equals": {
                      "field": "responseObject.type",
                      "value": "PullRequestEvent"
                    }
                  }
                ],
                "unique_identifiers": [
                  {
                    "hydration_method": "literal",
                    "value": "github"
                  },
                  {
                    "hydration_method": "endpoint",
                    "value": {
                      "endpoint": "repo",
                      "parameter_sources": {
                        "repo_name": {
                          "hydration_method": "translate",
                          "value": "name"
                        }
                      },
                      "value_location": [
                        [
                          "owner.login"
                        ]
                      ]
                    }
                  }
                ],
                "mapped_fields": {
                  "api_id": {
                    "hydration_method": "endpoint",
                    "value": {
                      "endpoint": "repo",
                      "parameter_sources": {
                        "repo_name": {
                          "hydration_method": "translate",
                          "value": "name"
                        }
                      },
                      "value_location": [
                        [
                          "owner.id"
                        ]
                      ]
                    }
                  },
                  "handle": {
                    "hydration_method": "endpoint",
                    "value": {
                      "endpoint": "repo",
                      "parameter_sources": {
                        "repo_name": {
                          "hydration_method": "translate",
                          "value": "name"
                        }
                      },
                      "value_location": [
                        [
                          "owner.login"
                        ]
                      ]
                    }
                  }
                }
              },
              "branch": {
                "hydrate_list_location": "responseObject.repo.owner",
                "conditions": [
                  {
                    "equals": {
                      "field": "responseObject.type",
                      "value": "CreateEvent"
                    }
                  },
                  {
                    "equals": {
                      "field": "responseObject.payload.ref_type",
                      "value": "branch"
                    }
                  }
                ],
                "unique_identifiers": [
                  {
                    "hydration_method": "literal",
                    "value": "github"
                  },
                  {
                    "hydration_method": "endpoint",
                    "value": {
                      "endpoint": "repo",
                      "parameter_sources": {
                        "repo_name": {
                          "hydration_method": "translate",
                          "value": "name"
                        }
                      },
                      "value_location": [
                        [
                          "owner.login"
                        ]
                      ]
                    }
                  }
                ],
                "mapped_fields": {
                  "api_id": {
                    "hydration_method": "endpoint",
                    "value": {
                      "endpoint": "repo",
                      "parameter_sources": {
                        "repo_name": {
                          "hydration_method": "translate",
                          "value": "name"
                        }
                      },
                      "value_location": [
                        [
                          "owner.id"
                        ]
                      ]
                    }
                  },
                  "handle": {
                    "hydration_method": "endpoint",
                    "value": {
                      "endpoint": "repo",
                      "parameter_sources": {
                        "repo_name": {
                          "hydration_method": "translate",
                          "value": "name"
                        }
                      },
                      "value_location": [
                        [
                          "owner.login"
                        ]
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "pagination": {
          "method": "rfc5988",
          "next_link_location": "rel=\"next\""
        }
      }
    }
  },
  "endpoints": {
    "events": {
      "call_method": "GET",
      "path": "users/[$1]/events",
      "additional_path_fields": [
        {
          "description": "ID of the user",
          "parameter": "user_id",
          "replace": "[$1]"
        }
      ],
      "parameter_descriptions": {
        "access_token": {
          "description": "OAuth2 access key",
          "hydration_method": "server",
          "value": "access_token"
        },
        "next_url": {
          "description": "The URL for the next page",
          "hydration_method": "translate",
          "value": "signal.endpoint_data.events.events.next_url"
        },
        "page": {
          "description": "The page number for the endpoint",
          "hydration_method": "translate",
          "value": "signal.endpoint_data.events.events.page"
        },
        "user_id": {
          "description": "The user's ID",
          "hydration_method": "translate",
          "value": "signal.signal_data.login"
        }
      },
      "header_descriptions": {
        "user_agent": {
          "description": "The user agent header",
          "header_name": "User-Agent",
          "header_value": "[$1]",
          "hydration_location": "signal.signal_data.login",
          "hydration_method": "reformat",
          "value": "[$1]"
        }
      }
    },
    "repo": {
      "call_method": "GET",
      "path": "repos/[$1]",
      "additional_path_fields": [
        {
          "description": "Repo name",
          "parameter": "repo_name",
          "replace": "[$1]"
        }
      ],
      "parameter_descriptions": {
        "access_token": {
          "description": "OAuth2 access key",
          "hydration_method": "server",
          "value": "access_token"
        },
        "repo_name": {
          "description": "Name of Repository",
          "hydration_method": "translate",
          "value": "responseObject.repo.name"
        }
      },
      "header_descriptions": {
        "user_agent": {
          "description": "The user agent header",
          "header_name": "User-Agent",
          "header_value": "[$1]",
          "hydration_location": "signal.signal_data.login",
          "hydration_method": "reformat",
          "value": "[$1]"
        }
      }
    }
  }
}