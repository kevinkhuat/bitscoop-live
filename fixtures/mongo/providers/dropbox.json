{
  "provider": {
    "provider_number": "000000000000000000000005",
    "name": "Dropbox",
    "client_callable": true,
    "domain": "https://api.dropbox.com/1",
    "backend_name": "dropbox-oauth2",
    "endpoint_wait_time": 0,
    "auth_backend": "social.backends.dropbox.DropboxOAuth2",
    "description": "Cloud storage solution",
    "tags": [
      "media",
      "productivity"
    ],
    "auth_type": 0
  },
  "event_sources": {
    "edits": {
      "description": "Edits made to your Dropbox files",
      "display_name": "Edits",
      "enabled_by_default": true,
      "endpoints": [
        "delta"
      ],
      "initial_mapping": "delta",
      "mappings": {
        "delta": {
          "returned_data_location": "entries",
          "type": "edit",
          "db_entry_mapping": {
            "data": {
              "main": {
                "unique_identifiers": [
                  {
                    "hydration_method": "translate",
                    "value": "permission.event_source.name"
                  },
                  {
                    "hydration_method": "translate",
                    "value": "responseObject.1.path"
                  },
                  {
                    "hydration_method": "translate",
                    "value": "responseObject.1.modified"
                  }
                ]
              }
            },
            "event": {
              "unique_identifiers": [
                {
                  "hydration_method": "translate",
                  "value": "responseObject.1.path"
                },
                {
                  "hydration_method": "translate",
                  "value": "responseObject.1.modified"
                }
              ],
              "mapped_fields": {
                "contact_interaction_type": {
                  "hydration_method": "literal",
                  "value": "with"
                },
                "datetime": {
                  "hydration_method": "translate",
                  "type": "string",
                  "value": "responseObject.1.modified"
                },
                "provider_name": {
                  "hydration_method": "literal",
                  "type": "string",
                  "value": "dropbox"
                }
              }
            },
            "content": {
              "files": {
                "conditions": [
                  {
                    "notEquals": {
                      "field": "responseObject.1.bytes",
                      "value": 0
                    }
                  }
                ],
                "unique_identifiers": [
                  {
                    "hydration_method": "literal",
                    "value": "dropbox"
                  },
                  {
                    "hydration_method": "translate",
                    "value": "responseObject.1.path"
                  }
                ],
                "mapped_fields": {
                  "file_extension": {
                    "hydration_method": "translate",
                    "value": "responseObject.1.mime_type"
                  },
                  "thumbnail": {
                    "hydration_method": "reformat",
                    "hydration_location": "responseObject.1.path",
                    "value": "https://content.dropboxapi.com/1/thumbnails/auto/[$1]"
                  },
                  "title": {
                    "hydration_method": "translate",
                    "value": "responseObject.1.path"
                  },
                  "type": {
                    "hydration_method": "literal",
                    "type": "string",
                    "value": "file"
                  }
                }
              }
            }
          }
        },
        "pagination": {
          "method": "cursor",
          "is_ascending": true,
          "next_cursor_location": "cursor",
          "start_cursor_parameter": "cursor",
          "cursor_value": "cursor",
          "check_if_more": "has_more"
        }
      }
    }
  },
  "endpoints": {
    "delta": {
      "call_method": "POST",
      "path": "delta",
      "parameter_descriptions": {
        "include_media_info": {
          "description": "Include additional information about photos and videos",
          "hydration_method": "literal",
          "value": true
        },
        "access_token": {
          "description": "OAuth2 access key",
          "hydration_method": "server",
          "value": "auth_data.access_token"
        },
        "cursor": {
          "description": "ID of the latest change found in the previous search",
          "hydration_method": "translate",
          "save_to_connection": {
            "location": "cursor"
          },
          "value": "connection.endpoint_data.delta.delta.cursor"
        }
      }
    }
  }
}